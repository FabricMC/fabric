{
  "schemaVersion": 1,
  "id": "fabric-resource-loader-v1",
  "name": "Fabric Resource Loader (v1)",
  "version": "${version}",
  "environment": "*",
  "license": "Apache-2.0",
  "icon": "assets/fabric-resource-loader-v1/icon.png",
  "contact": {
    "homepage": "https://fabricmc.net",
    "irc": "irc://irc.esper.net:6667/fabric",
    "issues": "https://github.com/FabricMC/fabric/issues",
    "sources": "https://github.com/FabricMC/fabric"
  },
  "authors": [
    "FabricMC"
  ],
  "depends": {
    "fabricloader": ">=0.14.19"
  },
  "description": "Asset and data resource loading.",
  "mixins": [
    "fabric-resource-loader-v1.mixins.json",
    {
      "config": "fabric-resource-loader-v1.client.mixins.json",
      "environment": "client"
    }
  ],
  "accessWidener": "fabric-resource-loader-v1.accesswidener",
  "custom": {
    "fabric-api:module-lifecycle": "stable",
    "loom:injected_interfaces": {
      "net/minecraft/class_310": ["net/fabricmc/fabric/api/resource/loader/v1/ResourceReloaderHolder"],
      "net/minecraft/class_5350": ["net/fabricmc/fabric/api/resource/loader/v1/ResourceReloaderHolder"],
      "net/minecraft/server/MinecraftServer": ["net/fabricmc/fabric/api/resource/loader/v1/ResourceReloaderHolder"]
    }
  }
}
